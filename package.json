  "dependencies": {
    "rxjs": "^5.0.0-beta.12"
  },
  "devDependencies": {
    "babel": "^6.5",
    "babel-core": "*",
    "babel-eslint": "*",
    "babel-loader": "^6.2",
    "babel-plugin-add-module-exports": "*",
    "babel-preset-es2015": "^6.14",
    "chai": "^3.5.0",
    "eslint": "*",
